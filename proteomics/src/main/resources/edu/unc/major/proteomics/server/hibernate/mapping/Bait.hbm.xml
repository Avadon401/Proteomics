<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="edu.unc.major.proteomics.share.model.Bait" table="Bait" catalog="signal">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="mutation" type="string">
            <column name="Mutation"/>
        </property>
      		
        <many-to-one name="gene" class="edu.unc.major.proteomics.share.model.NcbiGene" column="GeneID"/> 
        
		<set name="bands" lazy="true">
        	<key column="BaitID"/>
        	<one-to-many class="edu.unc.major.proteomics.share.model.Bait"/>
        </set> 
    </class>
</hibernate-mapping>