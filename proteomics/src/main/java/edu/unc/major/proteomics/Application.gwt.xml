<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
      <inherits name='com.google.gwt.activity.Activity' /> 
      
      <!-- Inherit other frameworks                       -->
      <inherits name='net.sf.gilead.Gilead4Gwt'/>

      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>

      <!-- Specify the app entry point class.                         -->
      <entry-point class='edu.unc.major.proteomics.client.Application'/>
      <servlet path="/tppProteinIndProteinService" class="edu.unc.major.proteomics.server.service.impl.TppProteinIndProteinServiceImpl"/> 
	  <servlet path="/tppProteinService" class="edu.unc.major.proteomics.server.service.impl.TppProteinServiceImpl"/> 
	  <servlet path="/ncbiGeneService" class="edu.unc.major.proteomics.server.service.impl.NcbiGeneServiceImpl"/> 
	  <servlet path="/constantService" class="edu.unc.major.proteomics.server.service.impl.ConstantServiceImpl"/>
	  <servlet path="/bandService" class="edu.unc.major.proteomics.server.service.impl.BandServiceImpl"/>
	  <servlet path="/siRNAValService" class="edu.unc.major.proteomics.server.service.impl.SiRNAValServiceImpl"/>
	  <servlet path="/exportService" class="edu.unc.major.proteomics.server.service.impl.ExportServiceImpl"/>
	  <servlet path="/export.csv" class="edu.unc.major.proteomics.server.servlet.ExportServlet"/>
	  
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />
      <stylesheet src="Autocomplete.css"/>
      
      <!-- Specify the paths to find translatable source -->
      <source path="client"/>
      <source path="share"/>
      
       <!-- Use ClientFactoryImpl by default -->
	   <replace-with class="edu.unc.major.proteomics.client.ClientFactoryImpl">
	   	<when-type-is class="edu.unc.major.proteomics.client.ClientFactory"/>
	   </replace-with>
</module>
